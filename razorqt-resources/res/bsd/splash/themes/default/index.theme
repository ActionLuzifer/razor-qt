# razorsplash default theme file

# splashpic is the backgroundpic of the splashscreen to use
# all paths relative to this file!
splashpic=default.png


# this one sets the splash timeout... the time when the splash automatically disappears
# its 4000ms in this case
splashtime=4000

#this is wether the splash should be hidden on click
#0 is no, 1 is yes

hideonclick=1
